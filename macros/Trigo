Sinus	macro
	clr.l	d0
	move.b	\1,d0
	mulu	pi,d0
	move	pi,d2
	divu	#2,d2
	divu	#1800,d0
	move	d0,d7
	clr.l	d0
	move	d7,d0
	add	d0,d1
	move	d7,d0
	mulu	d2,d0
	mulu	d2,d0
	divu	#6,d0
	sub	d0,d1
	move	d7,d0
	mulu	d2,d0
	mulu	d2,d0
	mulu	d2,d0
	mulu	d2,d0
	divu	#120,d0
	add	d0,d1
	move	d7,d0
	mulu	d2,d0
	mulu	d2,d0
	mulu	d2,d0
	mulu	d2,d0
	mulu	d2,d0
	mulu	d2,d0
	divu	#5040,d0
	sub	d0,d1
	move	d1,\2
	endm
